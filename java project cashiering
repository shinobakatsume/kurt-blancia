     else{
         JOptionPane.showMessageDialog(null,"INVALID INPUT");
         main = true;
     }
    }
    boolean here = true;
    while(here == true){
        int choice = Integer.parseInt(JOptionPane.showInputDialog("DO YOU WANT TO CHANGE ANOTHER PRODUCT'S NAME\n[1]YES\n[2]NO"));
        if(choice == 1){
            prNAME();
            
        }
        else if(choice == 2){
            adControl();
        }
        else{
            JOptionPane.showMessageDialog(null,"INVALID INPUT");
            here = true;
        }
    }
        
