            JOptionPane.showMessageDialog(null,"PRICE CHANGE IS SUCCSESFUL\nPRODUCT NAME : " +product[10]+ "\nUPDATED PRODUCT PRICE : "+ price[10] );
            main = false;
        }
        else if(price[10] == 0){
            JOptionPane.showMessageDialog(null, "PRODUCT SLOT IS EMPTY");
            main = true;
        } 
    }
