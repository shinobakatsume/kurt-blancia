        prCODE();
    }
    boolean code = true;
    boolean main = true, rept = true;
    while(main == true){
            
        transaction  = JOptionPane.showInputDialog(null, new JTextArea("AVAIABLE ITEMS \nCODE \tPRODUCT NAME \tPRICE\n"
                 +prodCODE[1] + "\t"  + product[1] + "\t"+ price[1] +"\n"
